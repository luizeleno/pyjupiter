<!-- Modal -->
<div class="modal fade" id="EELLOQ4087" tabindex="-1" role="dialog" aria-labelledby=" Termodin√¢mica Qu√≠mica Aplicada I" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header alert-dark">
                <h5 class="modal-title">LOQ4087</h5>
