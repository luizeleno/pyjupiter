<!-- Modal -->
<div class="modal fade" id="EELLOQ4073" tabindex="-1" role="dialog" aria-labelledby=" Qu√≠mica Geral II" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header alert-dark">
                <h5 class="modal-title">LOQ4073</h5>
