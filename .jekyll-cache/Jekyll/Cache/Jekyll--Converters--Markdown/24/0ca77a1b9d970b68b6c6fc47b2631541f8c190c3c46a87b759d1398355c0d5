<!-- Modal -->
<div class="modal fade" id="EELLOQ4100" tabindex="-1" role="dialog" aria-labelledby=" Fundamentos de Qu√≠mica para Engenharia I-B" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header alert-dark">
                <h5 class="modal-title">LOQ4100</h5>
