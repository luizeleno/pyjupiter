<!-- Modal -->
<div class="modal fade" id="EELLOB1056" tabindex="-1" role="dialog" aria-labelledby=" Introdu√ß√£o aos M√©todos Num√©ricos e Computacionais" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header alert-dark">
                <h5 class="modal-title">LOB1056</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
