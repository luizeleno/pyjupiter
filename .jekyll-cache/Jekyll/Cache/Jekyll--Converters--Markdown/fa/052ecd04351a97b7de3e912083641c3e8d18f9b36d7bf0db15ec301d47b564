<!-- Modal -->
<div class="modal fade" id="EELLOB1206" tabindex="-1" role="dialog" aria-labelledby=" Solos I" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header alert-dark">
                <h5 class="modal-title">LOB1206</h5>
