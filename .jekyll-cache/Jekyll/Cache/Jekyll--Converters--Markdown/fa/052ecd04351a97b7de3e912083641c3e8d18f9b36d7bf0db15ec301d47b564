<!-- Modal -->
<div class="modal fade" id="EELLOB1251" tabindex="-1" role="dialog" aria-labelledby=" Introdu√ß√£o a Inova√ß√£o Sistem√°tica com TRIZ" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header alert-dark">
                <h5 class="modal-title">LOB1251</h5>
