                        <tr>
                            <td>
                                <p>Turma: 20231B2</p>
                            </td>
                            <td>
                                <p>
                                    
                                         seg  14:00  18:00  (R) Gilberto Garcia Cortez 
                                        
                                    
                                </p>
                            </td>
                        </tr>
                        
