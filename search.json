layout: null
permalink: /search/search.json
---

{% assign pages = site.pages | where: 'disc', 'true' %}

[
  {% for post in pages %}
    {
      "title"    : "{{ post.title | escape }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "nome"     : "{{post.nome}}",
      "nomeascii"     : "{{post.nomeascii}}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
